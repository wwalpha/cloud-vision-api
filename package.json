{
  "name": "cloud-vision-api",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/wwalpha/cloud-vision-api.git",
  "license": "MIT",
  "scripts": {
    "start": "nodemon ./src/index.ts",
    "prebuild": "tsc",
    "build": "docker build -t vision .",
    "test": "ts-node test/test.ts"
  },
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.7",
    "@types/node": "^14.0.27",
    "dotenv": "^8.2.0",
    "nodemon": "^2.0.4",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@google-cloud/vision": "^3.0.1",
    "axios": "^1.0.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.18.1"
  },
  "nodemonConfig": {
    "watch": [
      "src/**/*.ts"
    ],
    "delay": 3,
    "ignore": [
      "node_modules/*"
    ],
    "execMap": {
      "ts": "ts-node -r tsconfig-paths/register -r dotenv/config"
    }
  }
}